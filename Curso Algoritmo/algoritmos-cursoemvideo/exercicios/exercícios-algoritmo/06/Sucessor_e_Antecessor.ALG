algoritmo "Sucessor_e_Antecessor"
var
    n: inteiro
inicio
    escreva("Informe o valor: ")
    //escreva("onde ser√° mostrado seu Sucessor e o seu Antecessor.")
    leia(n)
    suces <- (n + 1)
    antec <- n - 1
    escreva()
    //escreva("O Sucessor de ",n , " e ", suces)
    //escreva("O Antecessor de ",n , " e ", antec)
fimalgoritmo