algoritmo "Média"
var
   n1, n2, m: real 
inicio
      escreva("Digite a primeira nota: ")
      leia(n1)
      escreva("Digite sua segunda nota: ")
      leia(n2)
      escreva("Sua primeira nota foi ", n1, " e sua segunda nota foi ",n2 , " sendo assim sua média é ", (n1 + n2) / 2)
fimalgoritmo

